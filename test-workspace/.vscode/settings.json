                    "python",
                    "whoopy"
                ],
            },
            "thirdOption": {
                "emmet.excludeLanguages": [
                    "dart",
                    "flutter"
                ],
            },
        },
        "Simple value": {
            "editor10": {
                "_label": "my description",
                "editor.fontSize": 10,
            },
            "editor18": {
                "editor.fontSize": 18,
            },
            "editor36": {
                "editor.fontSize": 36,
            },
        },
    },
    "emmet.excludeLanguages": [
        "markdown",
        "javascriptreact"
