        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        ".serverless": true,
        "**/__pycache__": true,
        "**/.serverless": true,
        "**/dist": true,
        "**/node_modules": true,
        "yarn-error.log": true,
        "yarn.lock": true,
        "**/__tests__": true,
        "**/__mocks__": true,
        "**/__fixtures__": true,
        "**/*.spec.js": true,
